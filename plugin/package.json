{
  "name": "design-feed-plugin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node --no-deprecation ./node_modules/vite/bin/vite.js build && node scripts/inline-ui.js",
    "dev": "node --no-deprecation ./node_modules/vite/bin/vite.js build --watch",
    "build:prod": "node -e \"if(!process.env.VITE_BACKEND_URL){console.error('Defina VITE_BACKEND_URL (ex: VITE_BACKEND_URL=https://seu-projeto.vercel.app/api npm run build:prod)');process.exit(1)}\" && npm run build"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.92.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  }
}
